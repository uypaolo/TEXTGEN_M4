          </lhs>
          <rhs morphemeType="prefix" changeType="end" oldValue="s" newValue="es" />
          <stem stemType="doesn't change" beginOrEnd="y" />
          <description>hjkref</description>
        </rule>
        <rule name="morpho2" type="morphophonemic" status="false">
