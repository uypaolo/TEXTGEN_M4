<group name="Rules">
  <group name="Group 1">
    <group name="Subgroup1">
      <group name="nouns">
        <rule name="structural1" type="structural" status="false">
          <lhs>
            <component name="cl">
              <component name="np">
                <component name="n" concept="Melissa" sense="A" lexicon="Melissa" />
              </component>
              <component name="vp">
                <component name="v" concept="be" sense="A" />
              </component>
            </component>
          </lhs>
          <output>
            <component name="cl">
              <component name="np">
                <component name="n" concept="Melissa" sense="A" lexicon="Melissa">
                  <features>
                    <feature name="Number" value="Dual" />
                  </features>
                </component>
              </component>
              <component name="vp">
                <features>
                  <feature name="Sequence" value="First Coordinate" />
                </features>
                <component name="v" concept="be" sense="A" />
              </component>
            </component>
          </output>
          <rhs>
            <action type="edit feature" description="Set vp feature: Sequence to First Coordinate.">
              <editFeature target="-1">
                <feature name="Sequence" value="First Coordinate" />
              </editFeature>
            </action>
            <action type="edit feature" description="Set n: Melissa-A feature: Participant Tracking to First Mention.">
              <editFeature target="-1">
                <feature name="Participant Tracking" value="First Mention" />
              </editFeature>
            </action>
            <action type="edit feature" description="Set n: Melissa-A feature: Person to First.">
              <editFeature target="-1">
                <feature name="Person" value="First" />
              </editFeature>
            </action>
            <action type="edit feature" description="Set n: Melissa-A  feature: Number to Dual.">
              <editFeature target="-1">
                <feature name="Number" value="Dual" />
              </editFeature>
            </action>
          </rhs>
          <description />
        </rule>
        <rule name="spellout1" type="simple spellout" status="false">
          <lhs>
            <component name="n" concept="John" sense="A" />
          </lhs>
          <rhs type="simple spellout" subWord="jumo" form="base" />
          <description>etcccccccefrferg</description>
        </rule>
        <rule name="formselection1" type="form selection" status="false">
          <lhs>
            <component name="N" concept="John" sense="A" />
          </lhs>
          <form name="plural" />
          <description>hjkref</description>
        </rule>
      </group>
      <group name="verbs">
